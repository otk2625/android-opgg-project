<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="110dp">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="314dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="294dp" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_win_or_loss"
        android:layout_width="60dp"
        android:layout_height="match_parent"
        android:background="#5383E9"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView

            android:id="@+id/tv_win_or_loss"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:gravity="center"
            android:text="승"
            android:textColor="#FFFFFF"
            android:textSize="30dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/gray"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv_timestamp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="28dp"
            android:gravity="center"
            android:text="31:03"
            android:textColor="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/civ_championimg"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/ic_launcher_background"
        app:civ_border_color="#FFFFFF"
        app:civ_border_width="2dp"
        app:layout_constraintStart_toEndOf="@+id/cl_win_or_loss"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/iv_spell1"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginStart="8dp"
        app:layout_constraintStart_toEndOf="@+id/civ_championimg"
        app:layout_constraintTop_toTopOf="@+id/civ_championimg"
        app:srcCompat="@drawable/ic_launcher_background" />

    <ImageView
        android:id="@+id/iv_spell2"
        android:layout_width="27dp"
        android:layout_height="27dp"
        app:layout_constraintBottom_toBottomOf="@+id/civ_championimg"
        app:layout_constraintStart_toStartOf="@+id/iv_spell1"
        app:srcCompat="@drawable/ic_launcher_background" />

    <ImageView
        android:id="@+id/iv_item0"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toStartOf="@+id/iv_item1"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/cl_win_or_loss"
        app:layout_constraintTop_toBottomOf="@+id/civ_championimg"
        android:background="@color/noItem" />

    <ImageView
        android:id="@+id/iv_item1"
        android:layout_width="27dp"
        android:layout_height="27dp"
        app:layout_constraintEnd_toStartOf="@+id/iv_item2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/iv_item0"
        app:layout_constraintTop_toTopOf="@+id/iv_item0"
        android:background="@color/noItem" />

    <ImageView
        android:id="@+id/iv_item2"
        android:layout_width="27dp"
        android:layout_height="27dp"
        app:layout_constraintEnd_toStartOf="@+id/iv_item3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/iv_item1"
        app:layout_constraintTop_toTopOf="@+id/iv_item1"
        android:background="@color/noItem" />

    <ImageView
        android:id="@+id/iv_item3"
        android:layout_width="27dp"
        android:layout_height="27dp"
        app:layout_constraintEnd_toStartOf="@+id/iv_item4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/iv_item2"
        app:layout_constraintTop_toTopOf="@+id/iv_item2"
        android:background="@color/noItem" />

    <ImageView
        android:id="@+id/iv_item4"
        android:layout_width="27dp"
        android:layout_height="27dp"
        app:layout_constraintEnd_toStartOf="@+id/iv_item5"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/iv_item3"
        app:layout_constraintTop_toTopOf="@+id/iv_item3"
        android:background="@color/noItem" />

    <ImageView
        android:id="@+id/iv_item5"
        android:layout_width="27dp"
        android:layout_height="27dp"
        app:layout_constraintEnd_toStartOf="@+id/iv_item6"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/iv_item4"
        app:layout_constraintTop_toTopOf="@+id/iv_item4"
        android:background="@color/noItem" />

    <ImageView
        android:id="@+id/iv_item6"
        android:layout_width="27dp"
        android:layout_height="27dp"
        app:layout_constraintEnd_toStartOf="@+id/guideline4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/iv_item5"
        app:layout_constraintTop_toTopOf="@+id/iv_item5"
        android:background="@color/noItem" />

    <TextView
        android:id="@+id/tv_kill_solo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="5"
        android:textColor="@color/black"
        android:textSize="16dp"
        app:layout_constraintEnd_toStartOf="@+id/textView25"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/iv_item3"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_teamdragon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        android:textColor="#E91E1E"
        android:textSize="16dp"
        app:layout_constraintEnd_toStartOf="@+id/tv_team_lose_tower"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView25"
        app:layout_constraintTop_toTopOf="@+id/textView25" />

    <TextView
        android:id="@+id/tv_assist_solo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="14"
        android:textColor="@color/black"
        android:textSize="16dp"
        app:layout_constraintEnd_toStartOf="@+id/guideline6"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tv_team_lose_tower"
        app:layout_constraintTop_toTopOf="@+id/tv_team_lose_tower" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="/"
        android:textColor="@color/black"
        android:textSize="16dp"
        app:layout_constraintEnd_toStartOf="@+id/tv_teamdragon"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tv_kill_solo"
        app:layout_constraintTop_toTopOf="@+id/tv_kill_solo" />

    <TextView
        android:id="@+id/tv_team_lose_tower"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="/"
        android:textColor="@color/black"
        android:textSize="16dp"
        app:layout_constraintEnd_toStartOf="@+id/tv_assist_solo"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tv_teamdragon"
        app:layout_constraintTop_toTopOf="@+id/tv_teamdragon" />

    <TextView
        android:id="@+id/tv_game_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:text="솔랭"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.356" />

    <TextView
        android:id="@+id/tv_game_cration"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="7 시간전"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/tv_game_type"
        app:layout_constraintTop_toBottomOf="@+id/tv_game_type"
        app:layout_constraintVertical_bias="0.239" />

    <View
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="@color/gray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>